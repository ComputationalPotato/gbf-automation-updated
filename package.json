        "update": "yarn install && yarn upgrade-interactive --latest"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@tauri-apps/cli": "^1.0.0-beta.10",
        "@types/autosuggest-highlight": "^3.2.0",
        "@types/react": "^17.0.22",
        "@types/react-router-dom": "^5.3.3",
        "typescript": "^4.4.3"
    }
